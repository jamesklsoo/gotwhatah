<h1>Welcome to Got What Ah?</h1>
<%= link_to "sign in",  sign_in_path %>
<%= link_to "sign up", sign_up_path %>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Location</th>
      <th>Title</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.user_id %></td>
        <td><%= event.location_id %></td>
        <td><%= event.title %></td>
        <td><%= event.category %></td>
        <td><%= link_to "see this event", event_path(event.id) %></td>
     </tr>
    <% end %>
  </tbody>
</table>
