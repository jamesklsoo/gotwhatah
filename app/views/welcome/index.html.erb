<h1>Welcome to Got What Ah?</h1>
<%= link_to "sign in",  sign_in_path %>
<%= link_to "sign up", sign_up_path %>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.category %></td> 
      </tr>
    <% end %>
  </tbody>
</table>


