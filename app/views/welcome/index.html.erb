<h1>Welcome to Got What Ah</h1>
<%= link_to "sign in",  sign_in_path %>
<%= link_to "sign up", sign_up_path %>


<% @events.each do |t| %>
<tr> 
  <td><%= t.title %></td>
  <td><%= t.start_date %></td>
  <td><%= t.end_date %></td>
  <td><%= category %></td>
  <td><%= link_to "see this event", event_path(t.id) %></td>
</tr>
<% end %>
